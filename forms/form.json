          "type": "input",
          "title": "Your full name",
          "name": "name",
          "placeholder": "Type your name here...",
          "validations": {
            "type": "number",
            "list": [
              ">=2",
              "<      7.12",
              "integer"
            ]
          },
          "onError": "Expected an integer number between 2 and 7.12"
        },
        {
          "type": "radio",
          "title": "What province do you currently live in?",
          "subtitle": "If you don not live in Cuba, please specify Province or State",
          "name": "province",
          "items": [
            "Pinar del Rio",
            "Artemisa",
            "La Habana",
            "Mayabeque",
            "Isla de la Juventud",
            "Matanzas",
            "Cienfuegos",
            "Villa Clara",
            "Sancti Spiritus",
